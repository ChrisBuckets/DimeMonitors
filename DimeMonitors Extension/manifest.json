{
  "manifest_version": 2,

  "name": "Dime Monitors Extension",
  "version": "0.1.0",
  "description": "",

  "permissions": ["storage"],
  "icons": {
    "16": "DimeMonitorLogo.png",
    "48": "DimeMonitorLogo.png",
    "128": "DimeMonitorLogo.png"
  },
  "commands": {
    "toggle": {
      "description": "Toggle extension",
      "suggested__key": {
        "default": "Ctrl+Shift+A"
      }
    }
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": ["https://accounts.meetdapper.com/*"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
